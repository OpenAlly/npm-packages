{
  "name": "@openally/event-loop-monitor",
  "version": "1.0.0",
  "description": "Event Loop Monitor for Node.js to track event loop delay and utilization (framework agnostic)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "glob -c \"tsx --test\" \"./test/**/*.spec.ts\"",
    "coverage": "c8 --all --src ./src -r html npm test",
    "lint": "cross-env eslint src/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "GENTILHOMME Thomas <gentilhomme.thomas@gmail.com>",
  "license": "MIT"
}
